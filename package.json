		"@fastify/sensible": "^5.3.0",
		"@fastify/swagger": "^8.10.0",
		"@fastify/swagger-ui": "^1.9.3",
		"@prisma/client": "^5.3.1",
		"bcrypt": "^5.1.1",
		"dotenv": "^16.3.1",
		"fastify": "^4.23.2",
		"fastify-plugin": "^4.5.1",
